import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int sum=0;
        int a;
        do{
             a=sc.nextInt();
            sum=sum+a;
            
        }
        while(a!=-1);        
        System.out.printf("%d",sum+1);
    }
}
