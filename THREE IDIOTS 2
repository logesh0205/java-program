import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
    Scanner sc=new Scanner(System.in);
        int a=sc.nextInt();
        int b=sc.nextInt();
        int c=sc.nextInt();
        int d=sc.nextInt();
        float first=(a+c);
        float second=(b+d);
        float a1=first/2;
        float b1=second/2;
        System.out.printf("Binoy's house is located at (%.1f,%.01f)",a1,b1);
    }
}
