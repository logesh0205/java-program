import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int input=sc.nextInt();
        int sum=0;
        if(input>0){
            for(int i=1;i<input;i++)
        {
            if(input%i==0)
            {
                 sum=sum+i;
            }
           
        }
        if(sum==input)
        {
            System.out.printf("The Number %.1f is a Perfect Number.",(float)input);
        }
        else 
        {
            System.out.printf("The Number %.1f is a Not Perfect Number.",(float)input);
        }
        }
        
        else
        {
            System.out.println("Invalid Input");
        }
    }
}
